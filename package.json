{
  "name": "tsubasa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:vite": "vite",
    "dev:peer": "node src/peerServer/index.js",
    "dev:signaling": "node --expose-gc src/server/index.js",
    "dev:stun": "node src/js/stunServer/index.js",
    "dev:tts": "python src/server/python/tts_server.py",
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:peer\" \"npm run dev:signaling\" \"npm run dev:stun\" \"npm run dev:tts\"",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "peer": "^1.0.2",
    "peerjs": "^1.5.4",
    "vite": "^6.0.3",
    "vite-plugin-static-copy": "^2.2.0"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.1.2",
    "@ricky0123/vad-web": "^0.0.21",
    "dotenv": "^16.4.7",
    "node-stun": "^0.1.2",
    "onnxruntime-web": "^1.20.1",
    "openai": "^4.76.1",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "web-audio-peak-meter": "^3.1.0"
  }
}
